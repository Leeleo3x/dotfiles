{
    "editor.fontSize": 18,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "editor.wordWrap": "on",
    "cSpell.userWords": [
        "attackable",
        "blockchain",
        "bytecode",
        "concretization",
        "concretize",
        "deserialized",
        "ethereum",
        "etherscan",
        "satisfiability"
    ],
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pub",
        "python",
        "restructuredtext",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings",
        "**/*.bbl"
    ],
    "editor.tabSize": 2,
    "terminal.integrated.fontSize": 14,
    "files.autoSave": "afterDelay",
    "vim.useSystemClipboard": true,
    "languageServerHaskell.useCustomHieWrapperPath": "hie-wrapper",
    "languageServerHaskell.useCustomHieWrapper": true
}